@font-face {
  font-family: 'ProximaNova';
  src: url('../fonts/ProximaNova-Bold.ttf') format('truetype');
  src: url('../fonts/ProximaNova-Bold.eot?#iefix') format('embedded-opentype'),
          url('../fonts/ProximaNova-Bold.eot'),
          url('../fonts/ProximaNova-Bold.ttf') format('truetype'),
          url('../fonts/ProximaNova-Bold.svg#ProximaNova') format('svg');
  font-weight: bold;
  font-style: normal;
}

@font-face {
  font-family: 'ProximaNova';
  src: url('../fonts/ProximaNova-Extrabld.ttf') format('truetype');
  src: url('../fonts/ProximaNova-Extrabld.eot?#iefix') format('embedded-opentype'),
          url('../fonts/ProximaNova-Extrabld.eot'),
          url('../fonts/ProximaNova-Extrabld.ttf') format('truetype'),
          url('../fonts/ProximaNova-Extrabld.svg#ProximaNova') format('svg');
  font-weight: 800;
  font-style: normal;
}